{
  "name": "cocorig-ui",
  "version": "1.0.0",
  "author": "cocorig",
  "license": "MIT",
  "description": "UI Library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "source": "./src/components/index.ts",
      "import": {
        "types": "./dist/types/components/index.d.ts",
        "default": "./dist/esm/components/index.js"
      },
      "require": {
        "types": "./dist/types/components/index.d.ts",
        "default": "./dist/cjs/components/index.js"
      }
    },
    "./css": {
      "source": "./src/css/index.ts",
      "import": {
        "types": "./dist/types/css/index.d.ts",
        "default": "./dist/esm/css/index.js"
      },
      "require": {
        "types": "./dist/types/css/index.d.ts",
        "default": "./dist/cjs/css/index.js"
      }
    },
    "./foundation": {
      "source": "./src/foundation/index.ts",
      "import": {
        "types": "./dist/types/foundation/index.d.ts",
        "default": "./dist/esm/foundation/index.js"
      },
      "require": {
        "types": "./dist/types/foundation/index.d.ts",
        "default": "./dist/cjs/foundation/index.js"
      }
    },
    "./styled-system": {
      "source": "./src/styled-system/index.ts",
      "import": {
        "types": "./dist/types/styled-system/index.d.ts",
        "default": "./dist/esm/styled-system/index.js"
      },
      "require": {
        "types": "./dist/types/styled-system/index.d.ts",
        "default": "./dist/cjs/styled-system/index.js"
      }
    },
    "./hook": {
      "source": "./src/hook/index.ts",
      "import": {
        "types": "./dist/types/hook/index.d.ts",
        "default": "./dist/esm/hook/index.js"
      },
      "require": {
        "types": "./dist/types/hook/index.d.ts",
        "default": "./dist/cjs/hook/index.js"
      }
    },
    "./utils": {
      "source": "./src/utils/index.ts",
      "import": {
        "types": "./dist/types/utils/index.d.ts",
        "default": "./dist/esm/utils/index.js"
      },
      "require": {
        "types": "./dist/types/utils/index.d.ts",
        "default": "./dist/cjs/utils/index.js"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c ",
    "watch": "rollup -cw",
    "clean": "rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --cache --debug --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [
    "ui",
    "components",
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cocorig/cocorig-ui.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "^3.2.3",
    "@emotion/babel-plugin": "^11.13.5",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-webpack5": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.21.0",
    "@storybook/theming": "^8.4.7",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "babel-loader": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.29.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^8.4.7",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@emotion/react": ">=11.x",
    "@emotion/styled": ">=11.x"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.x",
    "@emotion/styled": ">=11.x",
    "react": ">=19.x",
    "react-dom": ">=19.x"
  }
}
